{
  "name": "v2hub",
  "version": "0.8.5.3",
  "description": "This project used React Native to build a V2EX mobile client application. The main goal was to build a React Native rapid development scaffold.",
  "main": "index.js",
  "private": false,
  "scripts": {
    "postinstall": "patch-package",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "pod": "pod-install",
    "lint:code": "eslint --max-warnings 100 . --ext .js,.jsx,.ts,.tsx",
    "lint:style": "prettier --check '**/*.{ts,js,jsx,tsx}'",
    "lint:style:fix": "prettier --write '**/*.{ts,js,jsx,tsx}'",
    "lint": "yarn lint:code && yarn lint:style",
    "lint:fix": "yarn lint:style:fix && yarn lint:code --fix",
    "prepare": "if [[ $NODE_ENV != \"production\" ]]; then husky install; fi && patch-package",
    "p": "plop",
    "upgrade": "react-native upgrade",
    "check-dependencies": "rnx-dep-check",
    "fix-dependencies": "rnx-dep-check --write",
    "npm:check": "npx npm-check-updates",
    "clean": "npx rimraf node_modules",
    "clean:cache": "npx rimraf node_modules/.cache",
    "debug": "open \"rndebugger://set-debugger-loc?host=localhost&port=8081\""
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:funnyzak/react-native-v2ex.git"
  },
  "keywords": [
    "react",
    "react-native",
    "v2ex",
    "react-native-v2ex",
    "js",
    "redux",
    "quick-start",
    "2022"
  ],
  "author": "Leon <silenceace@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/funnyzak/react-native-v2ex/issues"
  },
  "homepage": "https://v2hub.yycc.dev/",
  "dependencies": {
    "@bugsnag/react-native": "7.18.2",
    "@react-native-async-storage/async-storage": "^1.17.8",
    "@react-native-masked-view/masked-view": "^0.2.8",
    "@react-navigation/bottom-tabs": "^6.3.2",
    "@react-navigation/drawer": "^6.4.3",
    "@react-navigation/material-top-tabs": "^6.2.2",
    "@react-navigation/native": "^6.0.11",
    "@react-navigation/native-stack": "^6.9.1",
    "@redux-devtools/extension": "^3.2.2",
    "@types/react": "^18",
    "dayjs": "^1.11.6",
    "i18n-js": "^3.8.0",
    "lodash": "^4.17.21",
    "react": "18.1.0",
    "react-native": "0.70.4",
    "react-native-actions-sheet": "^0.8.10",
    "react-native-device-info": "^10.3.0",
    "react-native-easy-toast": "^2.0.0",
    "react-native-fast-image": "^8.6.3",
    "react-native-gesture-handler": "^2.8.0",
    "react-native-linear-gradient": "^2.6.2",
    "react-native-localize": "^2.2.2",
    "react-native-pager-view": "^6.0.2",
    "react-native-reanimated": "^2.10.0",
    "react-native-render-html": "^6.3.4",
    "react-native-safe-area-context": "^4.4.1",
    "react-native-screens": "^3.15.0",
    "react-native-skeleton-placeholder": "^5.0.0",
    "react-native-splash-screen": "^3.3.0",
    "react-native-tab-view": "^3.3.0",
    "react-native-toast-message": "^2.1.5",
    "react-native-webview": "^11.23.0",
    "react-redux": "^8.0.2",
    "redux": "^4.2.0",
    "redux-persist": "^6.0.0",
    "redux-persist-transform-filter": "^0.0.20",
    "redux-thunk": "^2.4.1"
  },
  "devDependencies": {
    "@babel/core": "7.19.3",
    "@babel/plugin-proposal-export-namespace-from": "7.18.9",
    "@babel/runtime": "7.20.1",
    "@react-native-community/eslint-config": "3.2.0",
    "@rnx-kit/dep-check": "1.13.4",
    "@types/i18n-js": "3.8.3",
    "@types/jest": "29.2.1",
    "@types/lodash": "4.14.187",
    "@types/react-native": "0.70.6",
    "@types/react-test-renderer": "18.0.0",
    "@typescript-eslint/eslint-plugin": "5.42.0",
    "@typescript-eslint/parser": "5.42.0",
    "babel-jest": "29.2.2",
    "babel-plugin-module-resolver": "4.1.0",
    "deprecated-react-native-prop-types": "2.3.0",
    "eslint": "8.26.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-react": "7.31.10",
    "eslint-plugin-react-native": "4.0.0",
    "husky": "8.0.1",
    "jest": "29.2.2",
    "lint-staged": "13.0.3",
    "metro-react-native-babel-preset": "0.73.3",
    "npm-check-updates": "16.3.16",
    "patch-package": "6.5.0",
    "plop": "3.1.1",
    "pod-install": "0.1.38",
    "postinstall-postinstall": "2.1.0",
    "prettier": "2.7.1",
    "react-devtools": "4.26.1",
    "react-devtools-core": "4.26.1",
    "react-test-renderer": "18.1.0",
    "rimraf": "3.0.2",
    "typescript": "4.8.4"
  },
  "resolutions": {
    "@types/react": "^18"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "engines": {
    "node": ">= 16.0.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix",
    "*.{js,tx,tsx,sass,less,css,md}": "prettier --write"
  }
}
